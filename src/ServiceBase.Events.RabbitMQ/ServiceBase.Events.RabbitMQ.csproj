<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <Authors>Russlan Akiev</Authors>
    <Company />
    <Product>ServiceBase.Events.RabbitMQ</Product>
    <Description>RabbitMQ event sink for ServiceBase.Events</Description>
    <PackageProjectUrl>https://github.com/aruss/ServiceBase</PackageProjectUrl>
    <Copyright>Copyright (c) Russlan Akiev. All rights reserved.</Copyright>
    <PackageTags>dotnet dotnet-core rabbitmq</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ServiceBase.Events.RabbitMQ.nuspec" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="RabbitMQ.Client" Version="5.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ServiceBase\ServiceBase.csproj" />
  </ItemGroup>

</Project>